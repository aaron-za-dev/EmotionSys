<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>

<GridPane fx:id="gvRoot" prefWidth="340.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
  <columnConstraints>
    <ColumnConstraints halignment="LEFT" hgrow="SOMETIMES" />
    <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
  </rowConstraints>
   <children>
      <Label fx:id="lblDevice" wrapText="true">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Label fx:id="lblSerial" textFill="#4a4a4a" wrapText="true" GridPane.rowIndex="1" />
      <Label fx:id="lblInObs" textAlignment="JUSTIFY" wrapText="true" GridPane.columnSpan="2" GridPane.rowIndex="2" />
      <Label fx:id="lblService" wrapText="true" GridPane.rowIndex="3" />
      <Label fx:id="lblStRev" wrapText="true" GridPane.rowIndex="4" />
      <Label fx:id="lblStFix" wrapText="true" GridPane.rowIndex="5" />
      <Label fx:id="lblFixObs" textAlignment="JUSTIFY" wrapText="true" GridPane.columnSpan="2" GridPane.rowIndex="6" />
      <Label fx:id="lblCost" alignment="CENTER" textAlignment="CENTER" wrapText="true" GridPane.columnIndex="1" />
   </children>
</GridPane>
